{
  "name": "what-up",
  "version": "1.0.0",
  "private": true,
  "main": "server",
  "dependencies": {
    "apollo-server-express": "^2.0.0",
    "body-parser": "^1.18.3",
    "cookie": "^0.3.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "graphql": "^0.13.2",
    "jsonwebtoken": "^8.3.0",
    "morgan": "^1.9.0",
    "pg": "^7.4.3"
  },
  "scripts": {
    "app": "react-scripts start",
    "app:build": "react-scripts build",
    "app:test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "server": "nodemon -w server -w server/index.js server -w .env",
    "server:run": "NODE_ENV=production node server",
    "server:test": "NODE_ENV=test mocha server/test/**.js -c",
    "server:test:windows": "set NODE_ENV=test&&mocha server/test/**.js -c",
    "dev": "npm-run-all -p server app",
    "start": "NODE_ENV=production node server",
    "test": "npm run server:test && npm run app:test",
    "test:local": "CI=true npm run server:test && npm run app:test",
    "test:local:windows": "set CI=true&&npm run server:test:windows && npm run app:test"
  },
  "devDependencies": {
    "apollo-cache-inmemory": "^1.3.0-beta.7",
    "apollo-client": "^2.3.8",
    "apollo-link": "^1.2.2",
    "apollo-link-error": "^1.1.0",
    "apollo-link-http": "^1.5.4",
    "apollo-link-ws": "^1.0.8",
    "apollo-utilities": "^1.0.18",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "enzyme": "^3.4.1",
    "enzyme-adapter-react-16": "^1.2.0",
    "enzyme-to-json": "^3.3.4",
    "graphql-tag": "^2.9.2",
    "jest-enzyme": "^6.0.3",
    "jsdom": "^11.12.0",
    "mocha": "^5.2.0",
    "node-fetch": "^2.2.0",
    "npm-run-all": "^4.1.3",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-apollo": "^2.1.11",
    "react-dom": "^16.4.2",
    "react-scripts": "1.1.4",
    "sinon": "^6.1.5"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "engines": {
    "node": "10.6.x"
  }
}
